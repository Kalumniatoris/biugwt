<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:db="urn:import:com.google.gwt.user.datepicker.client" 
	>
	
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel>
	<g:VerticalPanel>
		<g:HorizontalPanel>
			<g:Label text="Nazwa" />
			<g:TextBox ui:field="nameBox" />
			<g:Label ui:field="completionLabel1" />
		</g:HorizontalPanel>
		<g:HorizontalPanel>
			<g:Label text="Data" />
			<db:DateBox ui:field="dateBox"/>
			<g:Label ui:field="completionLabel2" />
		</g:HorizontalPanel>
		<g:HorizontalPanel>
			<g:Label text="Password" />
			<g:PasswordTextBox ui:field="passwordBox"/>
			<g:Label ui:field="completionLabel3" />
		</g:HorizontalPanel>
		<g:HorizontalPanel>
		<g:Button ui:field="btnSubmit" text="Submit"/>
		<g:Button ui:field="btnDelete" text="Delete"/>
		</g:HorizontalPanel>	
	</g:VerticalPanel>
	</g:HTMLPanel>

</ui:UiBinder> 